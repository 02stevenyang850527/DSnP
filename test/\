#include <iostream>
#include <string>
#include <sstream>

using namespace std;

int main()
{
	string str;
	cout << "press something: " ;
	cin >> str;
	string test;
	int num;
	cout << str << endl;
	istringstream s(str);
	while( (s >> str) || (s >> num) )
	{
		if (s >> str)
			cout << str << " ";
		else 
			cout << num << " ";
	}

	return 0;
}
